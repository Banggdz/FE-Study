    <H1>Bulb</H1>
    <img id="bulb"src="data:image/png;base64,">
    <button id="toggleButton" onclick="toggleBulb()">Turn on</button>

    <script>
        let isOn=true;

        function toggleBulb(){
            const bulb=document.getElementById("bulb");
            const button=document.getElementById("toggleButton");
            if(isOn){
                bulb.src="https://cdn.pixabay.com/photo/2021/03/31/15/14/bulb-6139751_960_720.png";
                button.innerHTML="Turn on";
            } else{
                bulb.src="data:image/png;base64,";
                button.innerHTML="Turn off";
            }
            isOn=!isOn;
        }
    </script>
</div>
